def keys_values_and_items():
     S={1:20, 2:40, 3:50, 4:60, 5:80, 6:100}
     S.keys()
     for y in S.keys():
           print('keys',y)
     P={'a':120, 'b':340, 'c':650, 'd':860, 'e':80, 'f':100}
     P.values()
     for z in P.values():
           print('values',z)
     A={3:30, 4:40, 9:50, 4:60, 7:78}
     A.items()
     for t in A.items():
           print('items',t)
            
